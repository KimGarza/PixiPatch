{
  "expo": {
    "name": "elemental_editor",
    "entryPoint": "_layout.tsx",
    "slug": "elemental_editor",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/tempSplash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#ffffff"
      },
      "package": "com.anonymous.elemental_editor"
    },
    "web": {
      "bundler": "metro",
      "output": "static"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      [
        "with-rn-image-crop-picker",
        {
          "PhotoLibraryUsageDescription": "Allow app XYZ to access your photos",
          "CameraUsageDescription": "Allow app XYZ to access your camera",
          "MicrophoneUsageDescription": "Allow app XYZ to access your microphone"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
